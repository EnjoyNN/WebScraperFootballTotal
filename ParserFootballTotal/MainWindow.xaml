                         Name="tbSecondTimeInAwayCleanSheets" Margin="5,5,0,5" Width="25" />
            </StackPanel>

            <StackPanel Grid.Row="6" Grid.Column="5" Orientation="Horizontal">
                <CheckBox Grid.Row="6" Grid.Column="5" x:Name="cbSecondTimeInAllBothFailedToScore" 
                      Content="Не заб Оба 2-й Т Всего" IsChecked="False"/>
