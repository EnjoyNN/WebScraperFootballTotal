                <TextBox PreviewTextInput="NumberValidTextBox" MaxLength="2"
                         LostFocus="DeleteWhitespacesAndSetDefaultWhenLostFocus"
                         Name="tbSecondTimeInHomeBothFailedToScore" Margin="5,5,0,5" Width="25" />
            </StackPanel>

            <StackPanel Grid.Row="8" Grid.Column="5" Orientation="Horizontal">
                <CheckBox Grid.Row="8" Grid.Column="5" x:Name="cbSecondTimeInAwayBothFailedToScore" 
                      Content="Не заб Оба 2-й Т Гости" IsChecked="False"/>
                <TextBox PreviewTextInput="NumberValidTextBox" MaxLength="2"
                         LostFocus="DeleteWhitespacesAndSetDefaultWhenLostFocus"
                         Name="tbSecondTimeInAwayBothFailedToScore" Margin="5,5,0,5" Width="25" />
            </StackPanel>

