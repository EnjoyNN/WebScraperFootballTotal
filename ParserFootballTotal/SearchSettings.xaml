        <Grid Name="ToolBar" Background="Transparent" Grid.Row="0" MouseDown="ToolBar_MouseDown">
            <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
                <Image Name="buttonX" Height="15" Width="15" Margin="0,2,10,0" MouseDown="buttonX_MouseDown">
                    <Image.Style>
                        <Style TargetType="{x:Type Image}">
                            <Setter Property="Source" Value="Source/buttonXNonActivePFT.png" />
                            <Style.Triggers>
                                <DataTrigger
                                    Binding="{Binding IsMouseOver,
                                                              ElementName=buttonX}"
                                    Value="True">
                                    <Setter Property="Source"
                                            Value="Source/buttonXActivePFT.png" />
                                </DataTrigger>
                            </Style.Triggers>
                        </Style>
                    </Image.Style>

                    <Image.Effect>
                        <BlurEffect Radius="1" />
                    </Image.Effect>
                </Image>
            </StackPanel>
            <Polygon Fill="White" Opacity="0.4"
                     Points="100,0,
                 110,20,
                 990,20,
                 1000,0" />

