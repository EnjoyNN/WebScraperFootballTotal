                <TextBlock TextAlignment="Center" Width="180">
                    Сохранить настройки<LineBreak />
                    в файл
                </TextBlock>
            </Button>
            <Button Name="LoadSettings" Grid.Row="3" Margin="20,0,0,10" Height="40" Width="180"
                    BorderBrush="Black" Background="White" Click="LoadSettings_Click">
                <TextBlock TextAlignment="Center" Width="180">
                    Загрузить настройки<LineBreak />
                    из файла
                </TextBlock>
            </Button>
        </StackPanel>

    </Grid>
</Window>